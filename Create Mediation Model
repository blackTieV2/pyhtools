      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def create_refined_mediation_diagram(beta_values, p_values, save_path=\"mediation_model_fixed.png\"):\n",
        "    \"\"\"\n",
        "    Create a mediation diagram with curved direct effect and properly labeled arrows.\n",
        "    \"\"\"\n",
        "    G = nx.DiGraph()\n",
        "\n",
        "    # Define node positions\n",
        "    pos = {\n",
        "        \"Psychopathy\": (-2, 0),\n",
        "        \"Resource Constraints\": (0, 1),\n",
        "        \"Risk-Taking\": (0, 0),\n",
        "        \"Avoiding Clarification\": (0, -1),\n",
        "        \"DevianceFreq\": (2, 0)\n",
        "    }\n",
        "\n",
        "    G.add_nodes_from(pos.keys())\n",
        "\n",
        "    # Define edges with their beta and p-values\n",
        "    edges = [\n",
        "        (\"Psychopathy\", \"Resource Constraints\"),\n",
        "        (\"Psychopathy\", \"Risk-Taking\"),\n",
        "        (\"Psychopathy\", \"Avoiding Clarification\"),\n",
        "        (\"Resource Constraints\", \"DevianceFreq\"),\n",
        "        (\"Risk-Taking\", \"DevianceFreq\"),\n",
        "        (\"Avoiding Clarification\", \"DevianceFreq\")\n",
        "    ]\n",
        "\n",
        "    for edge in edges:\n",
        "        beta = beta_values.get(f\"{edge[0]}-{edge[1]}\", 0)\n",
        "        p_value = p_values.get(f\"{edge[0]}-{edge[1]}\", 1)\n",
        "\n",
        "        # Color code based on p-value\n",
        "        if p_value < 0.01:\n",
        "            color = \"green\"\n",
        "        elif p_value < 0.05:\n",
        "            color = \"orange\"\n",
        "        else:\n",
        "            color = \"gray\"\n",
        "\n",
        "        G.add_edge(\n",
        "            edge[0], edge[1],\n",
        "            weight=beta,\n",
        "            color=color,\n",
        "            label=f\"β = {beta:.2f}, p = {p_value:.3f}\"\n",
        "        )\n",
        "\n",
        "    # Create the plot\n",
        "    plt.figure(figsize=(14, 10))\n",
        "    edge_colors = [G[u][v]['color'] for u, v in G.edges()]\n",
        "    edge_labels = nx.get_edge_attributes(G, 'label')\n",
        "\n",
        "    # Draw the network\n",
        "    nx.draw(\n",
        "        G, pos,\n",
        "        with_labels=True,\n",
        "        node_size=4000,\n",
        "        node_color=\"lightblue\",\n",
        "        edge_color=edge_colors,\n",
        "        font_size=12,\n",
        "        arrows=True\n",
        "    )\n",
        "    nx.draw_networkx_edge_labels(G, pos, edge_labels=edge_labels, font_size=10)\n",
        "\n",
        "    # Draw the curved arrow for the direct effect from Psychopathy to DevianceFreq\n",
        "    direct_effect_label = \"β = 0.23, p = 0.001\"\n",
        "    plt.annotate(\n",
        "        direct_effect_label,\n",
        "        xy=pos[\"DevianceFreq\"],        # arrow tip at the DevianceFreq node\n",
        "        xytext=pos[\"Psychopathy\"],     # label starting near the Psychopathy node\n",
        "        arrowprops=dict(\n",
        "            arrowstyle=\"->\",\n",
        "            color=\"blue\",\n",
        "            lw=1.5,\n",
        "            linestyle=\"dashed\",\n",
        "            connectionstyle=\"arc3,rad=-0.3\"  # negative rad draws it curving downward\n",
        "        ),\n",
        "        fontsize=10,\n",
        "        color=\"blue\"\n",
        "    )\n",
        "\n",
        "    plt.title(\"Mediation Model with Psychopathy\")\n",
        "    plt.savefig(save_path)\n",
        "    plt.show()\n",
        "\n",
        "# Beta and p-values\n",
        "beta_values = {\n",
        "    \"Psychopathy-Resource Constraints\": 0.31,\n",
        "    \"Psychopathy-Risk-Taking\": 0.45,\n",
        "    \"Psychopathy-Avoiding Clarification\": 0.32,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.20,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.41,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.25,\n",
        "    \"Psychopathy-DevianceFreq\": 0.23  # Direct effect\n",
        "}\n",
        "\n",
        "p_values = {\n",
        "    \"Psychopathy-Resource Constraints\": 0.001,\n",
        "    \"Psychopathy-Risk-Taking\": 0.001,\n",
        "    \"Psychopathy-Avoiding Clarification\": 0.001,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.020,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.001,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.015,\n",
        "    \"Psychopathy-DevianceFreq\": 0.001  # Direct effect\n",
        "}\n",
        "\n",
        "# Create the corrected diagram\n",
        "create_refined_mediation_diagram(beta_values, p_values)\n"
      ],
