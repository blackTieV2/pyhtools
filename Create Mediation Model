    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def create_refined_mediation_diagram(beta_values, p_values, save_path=\"mediation_model_fixed.png\"):\n",
        "    \"\"\"\n",
        "    Draws:\n",
        "      - Mediator edges (color-coded by p-value) with text labels that match line color\n",
        "      - A separate, curved direct arrow (blue, dashed) with its own label (including stars)\n",
        "    \"\"\"\n",
        "\n",
        "    # Helper function: get stars for effect size\n",
        "    def star_rating(beta):\n",
        "        # Strong (beta>0.20) => ***\n",
        "        # Moderate (0.10 <= beta <= 0.20) => **\n",
        "        # Weak (beta<0.10) => *\n",
        "        if beta > 0.20:\n",
        "            return \"***\"\n",
        "        elif beta >= 0.10:\n",
        "            return \"**\"\n",
        "        else:\n",
        "            return \"*\"\n",
        "\n",
        "    # Helper function: color-code by significance\n",
        "    def significance_color(p):\n",
        "        # Green if p < 0.01, Orange if p < 0.05, else Gray\n",
        "        if p < 0.01:\n",
        "            return \"green\"\n",
        "        elif p < 0.05:\n",
        "            return \"orange\"\n",
        "        else:\n",
        "            return \"gray\"\n",
        "\n",
        "    # Build a DiGraph for the mediator edges only (exclude direct effect)\n",
        "    G = nx.DiGraph()\n",
        "\n",
        "    pos = {\n",
        "        \"Psychopathy\": (-2, 0),\n",
        "        \"Resource Constraints\": (0, 1),\n",
        "        \"Risk-Taking\": (0, 0),\n",
        "        \"Avoiding Clarification\": (0, -1),\n",
        "        \"DevianceFreq\": (2, 0)\n",
        "    }\n",
        "    G.add_nodes_from(pos.keys())\n",
        "\n",
        "    # List of edges that are mediated paths (not the direct effect)\n",
        "    mediator_edges = [\n",
        "        (\"Psychopathy\", \"Resource Constraints\"),\n",
        "        (\"Psychopathy\", \"Risk-Taking\"),\n",
        "        (\"Psychopathy\", \"Avoiding Clarification\"),\n",
        "        (\"Resource Constraints\", \"DevianceFreq\"),\n",
        "        (\"Risk-Taking\", \"DevianceFreq\"),\n",
        "        (\"Avoiding Clarification\", \"DevianceFreq\")\n",
        "    ]\n",
        "\n",
        "    # Add each mediator edge with the correct color & label\n",
        "    for (src, dst) in mediator_edges:\n",
        "        beta = beta_values.get(f\"{src}-{dst}\", 0)\n",
        "        p_val = p_values.get(f\"{src}-{dst}\", 1)\n",
        "\n",
        "        line_color = significance_color(p_val)\n",
        "        stars = star_rating(beta)\n",
        "\n",
        "        # We'll store these in the Graph so we can draw lines first, then label them manually\n",
        "        G.add_edge(\n",
        "            src,\n",
        "            dst,\n",
        "            beta=beta,\n",
        "            p_val=p_val,\n",
        "            color=line_color,\n",
        "            style=\"solid\",\n",
        "            label=f\"β = {beta:.2f}{stars}, p = {p_val:.3f}\"\n",
        "        )\n",
        "\n",
        "    # Create the plot\n",
        "    plt.figure(figsize=(12, 8))\n",
        "\n",
        "    # 1) Draw the mediator nodes\n",
        "    nx.draw_networkx_nodes(G, pos, node_size=4000, node_color=\"lightblue\")\n",
        "\n",
        "    # 2) Draw the mediator edges (lines)\n",
        "    edge_colors = [G[u][v]['color'] for (u, v) in G.edges()]\n",
        "    nx.draw_networkx_edges(\n",
        "        G,\n",
        "        pos,\n",
        "        edgelist=G.edges(),\n",
        "        edge_color=edge_colors,\n",
        "        style='solid',\n",
        "        arrows=True\n",
        "    )\n",
        "\n",
        "    # 3) Draw the node labels\n",
        "    nx.draw_networkx_labels(G, pos, font_size=10)\n",
        "\n",
        "    # 4) Manually place the mediator edge labels in the middle of each edge\n",
        "    for (u, v) in G.edges():\n",
        "        # Midpoint coordinates\n",
        "        x_start, y_start = pos[u]\n",
        "        x_end, y_end = pos[v]\n",
        "        mid_x = (x_start + x_end) / 2\n",
        "        mid_y = (y_start + y_end) / 2\n",
        "\n",
        "        # Get the label text and color\n",
        "        label_text = G[u][v]['label']\n",
        "        label_color = G[u][v]['color']\n",
        "\n",
        "        # Place text at midpoint of the edge\n",
        "        plt.text(\n",
        "            mid_x, mid_y,\n",
        "            label_text,\n",
        "            fontsize=10,\n",
        "            color=label_color,\n",
        "            ha=\"center\",\n",
        "            va=\"center\"\n",
        "        )\n",
        "\n",
        "    # 5) Draw the DIRECT EFFECT as a separate curved arrow\n",
        "    direct_src, direct_dst = \"Psychopathy\", \"DevianceFreq\"\n",
        "    direct_beta = beta_values[ f\"{direct_src}-{direct_dst}\" ]\n",
        "    direct_p = p_values[ f\"{direct_src}-{direct_dst}\" ]\n",
        "\n",
        "    # We'll always draw the direct effect in BLUE\n",
        "    # (If you also want it color-coded by significance, just do the same approach)\n",
        "    psy_x, psy_y = pos[direct_src]\n",
        "    dev_x, dev_y = pos[direct_dst]\n",
        "\n",
        "    # 5a) Arrow itself (dashed, curved)\n",
        "    plt.annotate(\n",
        "        \"\",\n",
        "        xy=(dev_x, dev_y),     # arrow tip at DevianceFreq\n",
        "        xytext=(psy_x, psy_y), # arrow tail at Psychopathy\n",
        "        arrowprops=dict(\n",
        "            arrowstyle=\"->\",\n",
        "            color=\"blue\",\n",
        "            lw=1.5,\n",
        "            linestyle=\"dashed\",\n",
        "            connectionstyle=\"arc3,rad=-0.3\"\n",
        "        )\n",
        "    )\n",
        "\n",
        "    # 5b) Place the direct-effect label near the midpoint (above the curve)\n",
        "    direct_stars = star_rating(direct_beta)\n",
        "    direct_label = f\"β = {direct_beta:.2f}{direct_stars}, p = {direct_p:.3f}\"\n",
        "    mid_x_direct = (psy_x + dev_x) / 2\n",
        "    mid_y_direct = (psy_y + dev_y) / 2 + 0.2  # Increase/decrease 0.2 to move label\n",
        "\n",
        "    plt.text(\n",
        "        mid_x_direct, mid_y_direct,\n",
        "        direct_label,\n",
        "        fontsize=10,\n",
        "        color=\"blue\",  # match the arrow color\n",
        "        ha=\"center\",\n",
        "        va=\"center\"\n",
        "    )\n",
        "\n",
        "    plt.title(\"Mediation Model with Psychopathy\")\n",
        "    plt.axis(\"off\")\n",
        "    plt.savefig(save_path, dpi=300)\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "# Example usage:\n",
        "beta_values = {\n",
        "    \"Psychopathy-Resource Constraints\": 0.31,\n",
        "    \"Psychopathy-Risk-Taking\": 0.45,\n",
        "    \"Psychopathy-Avoiding Clarification\": 0.32,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.20,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.41,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.25,\n",
        "    \"Psychopathy-DevianceFreq\": 0.23  # Direct effect\n",
        "}\n",
        "\n",
        "p_values = {\n",
        "    \"Psychopathy-Resource Constraints\": 0.001,\n",
        "    \"Psychopathy-Risk-Taking\": 0.001,\n",
        "    \"Psychopathy-Avoiding Clarification\": 0.001,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.020,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.001,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.015,\n",
        "    \"Psychopathy-DevianceFreq\": 0.001  # Direct effect\n",
        "}\n",
        "\n",
        "create_refined_mediation_diagram(beta_values, p_values)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 675
        },
        "id": "GsKTbSe3HL_U",
        "outputId": "e0c2de0e-3108-4ded-e142-dd90b07718cd"
      },
