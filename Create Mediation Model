    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def create_refined_mediation_diagram(beta_values, p_values, save_path=\"mediation_model_fixed.png\"):\n",
        "    \"\"\"\n",
        "    Uses NetworkX for mediators + a separate curved arrow (Psychopathy->DevianceFreq).\n",
        "    We add * to show effect size based on beta magnitude.\n",
        "    \"\"\"\n",
        "    G = nx.DiGraph()\n",
        "\n",
        "    # Node positions\n",
        "    pos = {\n",
        "        \"Psychopathy\": (-2, 0),\n",
        "        \"Resource Constraints\": (0, 1),\n",
        "        \"Risk-Taking\": (0, 0),\n",
        "        \"Avoiding Clarification\": (0, -1),\n",
        "        \"DevianceFreq\": (2, 0)\n",
        "    }\n",
        "    G.add_nodes_from(pos.keys())\n",
        "\n",
        "    # Edges for mediator paths\n",
        "    mediator_edges = [\n",
        "        (\"Psychopathy\", \"Resource Constraints\"),\n",
        "        (\"Psychopathy\", \"Risk-Taking\"),\n",
        "        (\"Psychopathy\", \"Avoiding Clarification\"),\n",
        "        (\"Resource Constraints\", \"DevianceFreq\"),\n",
        "        (\"Risk-Taking\", \"DevianceFreq\"),\n",
        "        (\"Avoiding Clarification\", \"DevianceFreq\")\n",
        "    ]\n",
        "\n",
        "    # Helper function to get star rating based on beta magnitude\n",
        "    def get_star_label(beta):\n",
        "        if abs(beta) > 0.20:\n",
        "            return \"***\"  # Strong\n",
        "        elif abs(beta) >= 0.10:\n",
        "            return \"**\"   # Moderate\n",
        "        else:\n",
        "            return \"*\"    # Weak\n",
        "\n",
        "    # Add mediator edges to the graph\n",
        "    for (src, dst) in mediator_edges:\n",
        "        beta = beta_values.get(f\"{src}-{dst}\", 0)\n",
        "        p_val = p_values.get(f\"{src}-{dst}\", 1)\n",
        "\n",
        "        # Determine color by p-value\n",
        "        if p_val < 0.01:\n",
        "            color = \"green\"\n",
        "        elif p_val < 0.05:\n",
        "            color = \"orange\"\n",
        "        else:\n",
        "            color = \"gray\"\n",
        "\n",
        "        star_label = get_star_label(beta)  # Get the \"***\"/\"**\"/\"*\" marker\n",
        "\n",
        "        G.add_edge(\n",
        "            src, dst,\n",
        "            weight=beta,\n",
        "            color=color,\n",
        "            style=\"solid\",\n",
        "            # Include the star label in the edge label\n",
        "            label=f\"β = {beta:.2f}{star_label}, p = {p_val:.3f}\"\n",
        "        )\n",
        "\n",
        "    plt.figure(figsize=(12, 8))\n",
        "\n",
        "    # Draw nodes\n",
        "    nx.draw_networkx_nodes(G, pos, node_size=4000, node_color=\"lightblue\")\n",
        "\n",
        "    # Draw mediator edges\n",
        "    edge_colors = [G[u][v]['color'] for (u, v) in G.edges()]\n",
        "    nx.draw_networkx_edges(\n",
        "        G, pos,\n",
        "        edgelist=G.edges(),\n",
        "        edge_color=edge_colors,\n",
        "        style='solid',\n",
        "        arrows=True\n",
        "    )\n",
        "\n",
        "    # Node labels\n",
        "    nx.draw_networkx_labels(G, pos, font_size=10)\n",
        "\n",
        "    # Mediator edge labels (NetworkX automatically centers them)\n",
        "    edge_labels = nx.get_edge_attributes(G, 'label')\n",
        "    nx.draw_networkx_edge_labels(G, pos, edge_labels=edge_labels, font_size=10)\n",
        "\n",
        "    # Now add the DIRECT EFFECT as a separate curved arrow\n",
        "    direct_src, direct_dst = \"Psychopathy\", \"DevianceFreq\"\n",
        "    direct_beta = beta_values[\"Psychopathy-DevianceFreq\"]\n",
        "    direct_p = p_values[\"Psychopathy-DevianceFreq\"]\n",
        "\n",
        "    # (1) Draw the curved arrow with annotate(), no text\n",
        "    psy_x, psy_y = pos[direct_src]\n",
        "    dev_x, dev_y = pos[direct_dst]\n",
        "\n",
        "    plt.annotate(\n",
        "        \"\",\n",
        "        xy=(dev_x, dev_y),      # arrow tip at DevianceFreq\n",
        "        xytext=(psy_x, psy_y),  # arrow tail at Psychopathy\n",
        "        arrowprops=dict(\n",
        "            arrowstyle=\"->\",\n",
        "            color=\"blue\",\n",
        "            lw=1.5,\n",
        "            linestyle=\"dashed\",\n",
        "            connectionstyle=\"arc3,rad=-0.3\"  # negative rad => curve arcs downward\n",
        "        )\n",
        "    )\n",
        "\n",
        "    # (2) Place the direct-effect label near the midpoint of that arc\n",
        "    # Include the star marker\n",
        "    direct_star = get_star_label(direct_beta)\n",
        "    mid_x = (psy_x + dev_x) / 2\n",
        "    mid_y = (psy_y + dev_y) / 2 + 0.3   # tweak this offset to move label up/down\n",
        "\n",
        "    plt.text(\n",
        "        mid_x, mid_y,\n",
        "        f\"β = {direct_beta:.2f}{direct_star}, p = {direct_p:.3f}\",\n",
        "        fontsize=10,\n",
        "        color=\"blue\",\n",
        "        ha=\"center\"\n",
        "    )\n",
        "\n",
        "    plt.title(\"Mediation Model with Psychopathy\")\n",
        "    plt.axis(\"off\")\n",
        "    plt.savefig(save_path, dpi=300)\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "# Example beta and p-values\n",
        "beta_values = {\n",
        "    \"Psychopathy-Resource Constraints\": 0.31,\n",
        "    \"Psychopathy-Risk-Taking\": 0.45,\n",
        "    \"Psychopathy-Avoiding Clarification\": 0.32,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.20,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.41,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.25,\n",
        "    \"Psychopathy-DevianceFreq\": 0.23  # direct effect\n",
        "}\n",
        "\n",
        "p_values = {\n",
        "    \"Psychopathy-Resource Constraints\": 0.001,\n",
        "    \"Psychopathy-Risk-Taking\": 0.001,\n",
        "    \"Psychopathy-Avoiding Clarification\": 0.001,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.020,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.001,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.015,\n",
        "    \"Psychopathy-DevianceFreq\": 0.001  # direct effect\n",
        "}\n",
        "\n",
        "create_refined_mediation_diagram(beta_values, p_values)\n"
      ],
      "metadata": {
