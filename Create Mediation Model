    {
      "cell_type": "code",
      "source": [],
      "metadata": {
        "id": "rAfbyZGQO75l"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def create_machiavellianism_mediation_diagram(beta_values, p_values, save_path=\"mediation_model_machiavellianism.png\"):\n",
        "    \"\"\"\n",
        "    Generate a mediation diagram for Machiavellianism with three mediators:\n",
        "        - Resource Constraints\n",
        "        - Quick Results\n",
        "        - Avoid Clarification\n",
        "    And DevianceFreq as the outcome.\n",
        "\n",
        "    Uses standard asterisk notation for p-values:\n",
        "        * p < .05\n",
        "        ** p < .01\n",
        "        *** p < .001\n",
        "\n",
        "    Also color-codes edges by significance (green/orange/gray).\n",
        "    \"\"\"\n",
        "\n",
        "    # 1. Helper functions: star notation & color by p-value\n",
        "    def get_star_label_from_p(p_value):\n",
        "        if p_value < 0.001:\n",
        "            return \"***\"\n",
        "        elif p_value < 0.01:\n",
        "            return \"**\"\n",
        "        elif p_value < 0.05:\n",
        "            return \"*\"\n",
        "        else:\n",
        "            return \"\"\n",
        "\n",
        "    def get_edge_color_from_p(p_value):\n",
        "        if p_value < 0.01:\n",
        "            return \"green\"\n",
        "        elif p_value < 0.05:\n",
        "            return \"orange\"\n",
        "        else:\n",
        "            return \"gray\"\n",
        "\n",
        "    # 2. Create a directed graph\n",
        "    G = nx.DiGraph()\n",
        "\n",
        "    # 3. Node positions (adjust as needed for aesthetics)\n",
        "    pos = {\n",
        "        \"Machiavellianism\":     (-2,  0),\n",
        "        \"Resource Constraints\": ( 0,  1),\n",
        "        \"Quick Results\":        ( 0,  0),\n",
        "        \"Avoid Clarification\":  ( 0, -1),\n",
        "        \"DevianceFreq\":         ( 2,  0)\n",
        "    }\n",
        "    G.add_nodes_from(pos.keys())\n",
        "\n",
        "    # 4. Define your mediator edges\n",
        "    mediator_edges = [\n",
        "        (\"Machiavellianism\", \"Resource Constraints\"),\n",
        "        (\"Machiavellianism\", \"Quick Results\"),\n",
        "        (\"Machiavellianism\", \"Avoid Clarification\"),\n",
        "        (\"Resource Constraints\", \"DevianceFreq\"),\n",
        "        (\"Quick Results\", \"DevianceFreq\"),\n",
        "        (\"Avoid Clarification\", \"DevianceFreq\")\n",
        "    ]\n",
        "\n",
        "    # 5. Add edges with labels & colors\n",
        "    for (src, dst) in mediator_edges:\n",
        "        beta = beta_values.get(f\"{src}-{dst}\", 0.0)\n",
        "        p_val = p_values.get(f\"{src}-{dst}\", 1.0)\n",
        "\n",
        "        stars = get_star_label_from_p(p_val)\n",
        "        color = get_edge_color_from_p(p_val)\n",
        "        label = f\"β = {beta:.2f}{stars}, p = {p_val:.3f}\"\n",
        "\n",
        "        G.add_edge(src, dst, weight=beta, color=color, label=label)\n",
        "\n",
        "    # 6. Draw the figure\n",
        "    plt.figure(figsize=(12, 8))\n",
        "    nx.draw_networkx_nodes(G, pos, node_size=4000, node_color=\"lightblue\")\n",
        "\n",
        "    edge_colors = [G[u][v]['color'] for u, v in G.edges()]\n",
        "    nx.draw_networkx_edges(G, pos, edge_color=edge_colors, arrows=True)\n",
        "\n",
        "    nx.draw_networkx_labels(G, pos, font_size=10)\n",
        "    edge_labels = nx.get_edge_attributes(G, 'label')\n",
        "    nx.draw_networkx_edge_labels(G, pos, edge_labels=edge_labels, font_size=10)\n",
        "\n",
        "    # 7. Add direct effect (Machiavellianism -> DevianceFreq) as a curved dashed arrow\n",
        "    direct_src, direct_dst = \"Machiavellianism\", \"DevianceFreq\"\n",
        "    direct_beta = beta_values.get(f\"{direct_src}-{direct_dst}\", 0.0)\n",
        "    direct_p = p_values.get(f\"{direct_src}-{direct_dst}\", 1.0)\n",
        "    direct_stars = get_star_label_from_p(direct_p)\n",
        "\n",
        "    src_x, src_y = pos[direct_src]\n",
        "    dst_x, dst_y = pos[direct_dst]\n",
        "\n",
        "    plt.annotate(\"\",\n",
        "                 xy=(dst_x, dst_y),\n",
        "                 xytext=(src_x, src_y),\n",
        "                 arrowprops=dict(arrowstyle=\"->\",\n",
        "                                 color=\"blue\",\n",
        "                                 lw=1.5,\n",
        "                                 linestyle=\"dashed\",\n",
        "                                 connectionstyle=\"arc3,rad=-0.3\"))\n",
        "\n",
        "    # Position the label near the midpoint\n",
        "    mid_x = (src_x + dst_x) / 2\n",
        "    mid_y = (src_y + dst_y) / 2 + 0.3\n",
        "\n",
        "    plt.text(mid_x, mid_y,\n",
        "             f\"β = {direct_beta:.2f}{direct_stars}, p = {direct_p:.3f}\",\n",
        "             fontsize=10, color=\"blue\", ha=\"center\")\n",
        "\n",
        "    plt.title(\"Mediation Model with Machiavellianism\")\n",
        "    plt.axis(\"off\")\n",
        "    plt.savefig(save_path, dpi=300)\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "# 8. EXAMPLE: Fill in with your final betas & p-values from the new Machiavellianism results tables\n",
        "beta_values_mach = {\n",
        "    \"Machiavellianism-Resource Constraints\":  0.06,\n",
        "    \"Machiavellianism-Quick Results\":         0.14,\n",
        "    \"Machiavellianism-Avoid Clarification\":   0.07,\n",
        "    \"Resource Constraints-DevianceFreq\":      0.18,\n",
        "    \"Quick Results-DevianceFreq\":             0.25,\n",
        "    \"Avoid Clarification-DevianceFreq\":       0.15,\n",
        "    # Direct effect:\n",
        "    \"Machiavellianism-DevianceFreq\":          0.02\n",
        "}\n",
        "\n",
        "p_values_mach = {\n",
        "    \"Machiavellianism-Resource Constraints\":  0.010,\n",
        "    \"Machiavellianism-Quick Results\":         0.002,\n",
        "    \"Machiavellianism-Avoid Clarification\":   0.050,\n",
        "    \"Resource Constraints-DevianceFreq\":      0.002,\n",
        "    \"Quick Results-DevianceFreq\":             0.002,\n",
        "    \"Avoid Clarification-DevianceFreq\":       0.037,\n",
        "    # Direct effect:\n",
        "    \"Machiavellianism-DevianceFreq\":          0.789\n",
        "}\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    create_machiavellianism_mediation_diagram(\n",
        "        beta_values_mach,\n",
        "        p_values_mach,\n",
        "        save_path=\"mediation_model_machiavellianism.png\"\n",
        "    )\n"
