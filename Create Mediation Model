      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def create_psychopathy_mediation_diagram(beta_values, p_values, save_path=\"mediation_model_psychopathy.png\"):\n",
        "    \"\"\"\n",
        "    Generate a mediation diagram for Psychopathy with three mediators:\n",
        "        - Resource Constraints\n",
        "        - Quick Results\n",
        "        - Avoid Clarification\n",
        "    And DevianceFreq as the outcome.\n",
        "\n",
        "    Star notation (strict APA):\n",
        "        *** if p <= .001\n",
        "        **  if p <= .01\n",
        "        *   if p <= .05\n",
        "        no star if p > .05\n",
        "\n",
        "    Color-coding by p-value:\n",
        "        - Green if p <= .01\n",
        "        - Orange if p <= .05 (and > .01)\n",
        "        - Gray if p > .05\n",
        "    \"\"\"\n",
        "\n",
        "    # 1. Asterisk notation: note use of <= for thresholds\n",
        "    def get_star_label_from_p(p_value):\n",
        "        if p_value <= 0.001:\n",
        "            return \"***\"\n",
        "        elif p_value <= 0.01:\n",
        "            return \"**\"\n",
        "        elif p_value <= 0.05:\n",
        "            return \"*\"\n",
        "        else:\n",
        "            return \"\"\n",
        "\n",
        "    # 2. Edge color by p-value\n",
        "    def get_edge_color_from_p(p_value):\n",
        "        if p_value <= 0.01:\n",
        "            return \"green\"\n",
        "        elif p_value <= 0.05:\n",
        "            return \"orange\"\n",
        "        else:\n",
        "            return \"gray\"\n",
        "\n",
        "    # 3. Create directed graph\n",
        "    G = nx.DiGraph()\n",
        "\n",
        "    # 4. Node positions\n",
        "    pos = {\n",
        "        \"Psychopathy\":         (-2,  0),\n",
        "        \"Resource Constraints\": (0,  1),\n",
        "        \"Quick Results\":        (0,  0),\n",
        "        \"Avoid Clarification\":  (0, -1),\n",
        "        \"DevianceFreq\":        ( 2,  0)\n",
        "    }\n",
        "    G.add_nodes_from(pos.keys())\n",
        "\n",
        "    # 5. Define mediator edges\n",
        "    mediator_edges = [\n",
        "        (\"Psychopathy\", \"Resource Constraints\"),\n",
        "        (\"Psychopathy\", \"Quick Results\"),\n",
        "        (\"Psychopathy\", \"Avoid Clarification\"),\n",
        "        (\"Resource Constraints\", \"DevianceFreq\"),\n",
        "        (\"Quick Results\", \"DevianceFreq\"),\n",
        "        (\"Avoid Clarification\", \"DevianceFreq\")\n",
        "    ]\n",
        "\n",
        "    # 6. Add edges with the correct betas, p-values, stars, and colors\n",
        "    for (src, dst) in mediator_edges:\n",
        "        beta = beta_values.get(f\"{src}-{dst}\", 0.0)\n",
        "        p_val = p_values.get(f\"{src}-{dst}\", 1.0)\n",
        "\n",
        "        stars = get_star_label_from_p(p_val)\n",
        "        color = get_edge_color_from_p(p_val)\n",
        "        label = f\"β = {beta:.2f}{stars}, p = {p_val:.3f}\"\n",
        "\n",
        "        G.add_edge(src, dst, weight=beta, color=color, label=label)\n",
        "\n",
        "    # 7. Draw the figure\n",
        "    plt.figure(figsize=(12, 8))\n",
        "    nx.draw_networkx_nodes(G, pos, node_size=4000, node_color=\"lightblue\")\n",
        "\n",
        "    edge_colors = [G[u][v]['color'] for u, v in G.edges()]\n",
        "    nx.draw_networkx_edges(G, pos, edge_color=edge_colors, arrows=True)\n",
        "\n",
        "    nx.draw_networkx_labels(G, pos, font_size=10)\n",
        "    edge_labels = nx.get_edge_attributes(G, 'label')\n",
        "    nx.draw_networkx_edge_labels(G, pos, edge_labels=edge_labels, font_size=10)\n",
        "\n",
        "    # 8. Direct effect: Psychopathy -> DevianceFreq (dashed arrow)\n",
        "    direct_src, direct_dst = \"Psychopathy\", \"DevianceFreq\"\n",
        "    direct_beta = beta_values.get(f\"{direct_src}-{direct_dst}\", 0.0)\n",
        "    direct_p = p_values.get(f\"{direct_src}-{direct_dst}\", 1.0)\n",
        "\n",
        "    direct_stars = get_star_label_from_p(direct_p)\n",
        "\n",
        "    src_x, src_y = pos[direct_src]\n",
        "    dst_x, dst_y = pos[direct_dst]\n",
        "\n",
        "    plt.annotate(\"\",\n",
        "                 xy=(dst_x, dst_y),\n",
        "                 xytext=(src_x, src_y),\n",
        "                 arrowprops=dict(arrowstyle=\"->\", color=\"blue\", lw=1.5,\n",
        "                                 linestyle=\"dashed\", connectionstyle=\"arc3,rad=-0.3\"))\n",
        "\n",
        "    # Position direct-effect label near midpoint, offset upward\n",
        "    mid_x = (src_x + dst_x) / 2\n",
        "    mid_y = (src_y + dst_y) / 2 + 0.3\n",
        "\n",
        "    plt.text(mid_x, mid_y,\n",
        "             f\"β = {direct_beta:.2f}{direct_stars}, p = {direct_p:.3f}\",\n",
        "             fontsize=10, color=\"blue\", ha=\"center\")\n",
        "\n",
        "    plt.title(\"Mediation Model with Psychopathy\")\n",
        "    plt.axis(\"off\")\n",
        "    plt.savefig(save_path, dpi=300)\n",
        "    plt.show()\n",
        "\n",
        "# -----------------------------------------------------------------------------\n",
        "# Final \"strict APA\" values for Psychopathy model, as per your tables.\n",
        "# Note: p=0.01 => **, p=0.005 => **, etc.\n",
        "# -----------------------------------------------------------------------------\n",
        "\n",
        "beta_values_psychopathy = {\n",
        "    \"Psychopathy-Resource Constraints\":   0.14,\n",
        "    \"Psychopathy-Quick Results\":          0.31,\n",
        "    \"Psychopathy-Avoid Clarification\":    0.11,\n",
        "    \"Resource Constraints-DevianceFreq\":  0.17,\n",
        "    \"Quick Results-DevianceFreq\":         0.21,\n",
        "    \"Avoid Clarification-DevianceFreq\":   0.15,\n",
        "    # Direct effect:\n",
        "    \"Psychopathy-DevianceFreq\":           0.23\n",
        "}\n",
        "\n",
        "p_values_psychopathy = {\n",
        "    \"Psychopathy-Resource Constraints\":   0.003,  # < .01 => **\n",
        "    \"Psychopathy-Quick Results\":          0.001,  # < .01 => **\n",
        "    \"Psychopathy-Avoid Clarification\":    0.026,  # < .05 => *\n",
        "    \"Resource Constraints-DevianceFreq\":  0.008,  # < .01 => **\n",
        "    \"Quick Results-DevianceFreq\":         0.010,  # <= .01 => ** (strict APA)\n",
        "    \"Avoid Clarification-DevianceFreq\":   0.037,  # < .05 => *\n",
        "    # Direct effect:\n",
        "    \"Psychopathy-DevianceFreq\":           0.005   # < .01 => **\n",
        "}\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    create_psychopathy_mediation_diagram(\n",
        "        beta_values_psychopathy,\n",
        "        p_values_psychopathy,\n",
        "        save_path=\"mediation_model_psychopathy.png\"\n",
        "    )\n"
