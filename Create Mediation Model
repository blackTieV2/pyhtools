      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def create_narcissism_mediation_diagram(beta_values, p_values, save_path=\"mediation_model_narcissism.png\"):\n",
        "    \"\"\"\n",
        "    Generate a mediation diagram for Narcissism with three mediators:\n",
        "        - Resource Constraints\n",
        "        - Quick Results\n",
        "        - Avoid Clarification\n",
        "    And DevianceFreq as the outcome.\n",
        "\n",
        "    Uses standard asterisk notation for p-values:\n",
        "        *  p < .05\n",
        "        ** p < .01\n",
        "        *** p < .001\n",
        "\n",
        "    Also color-codes edges by significance:\n",
        "        - Green for p < .01\n",
        "        - Orange for p < .05 (and >= .01)\n",
        "        - Gray for p >= .05\n",
        "    \"\"\"\n",
        "\n",
        "    def get_star_label_from_p(p_value):\n",
        "        if p_value < 0.001:\n",
        "            return \"***\"\n",
        "        elif p_value < 0.01:\n",
        "            return \"**\"\n",
        "        elif p_value < 0.05:\n",
        "            return \"*\"\n",
        "        else:\n",
        "            return \"\"\n",
        "\n",
        "    def get_edge_color_from_p(p_value):\n",
        "        if p_value < 0.01:\n",
        "            return \"green\"\n",
        "        elif p_value < 0.05:\n",
        "            return \"orange\"\n",
        "        else:\n",
        "            return \"gray\"\n",
        "\n",
        "    G = nx.DiGraph()\n",
        "\n",
        "    # Node positions (adjust as you like for clarity)\n",
        "    pos = {\n",
        "        \"Narcissism\":          (-2,  0),\n",
        "        \"Resource Constraints\": ( 0,  1),\n",
        "        \"Quick Results\":        ( 0,  0),\n",
        "        \"Avoid Clarification\":  ( 0, -1),\n",
        "        \"DevianceFreq\":         ( 2,  0)\n",
        "    }\n",
        "    G.add_nodes_from(pos.keys())\n",
        "\n",
        "    # Mediator edges\n",
        "    mediator_edges = [\n",
        "        (\"Narcissism\", \"Resource Constraints\"),\n",
        "        (\"Narcissism\", \"Quick Results\"),\n",
        "        (\"Narcissism\", \"Avoid Clarification\"),\n",
        "        (\"Resource Constraints\", \"DevianceFreq\"),\n",
        "        (\"Quick Results\", \"DevianceFreq\"),\n",
        "        (\"Avoid Clarification\", \"DevianceFreq\")\n",
        "    ]\n",
        "\n",
        "    # Add edges with the correct labels & colors\n",
        "    for (src, dst) in mediator_edges:\n",
        "        beta = beta_values.get(f\"{src}-{dst}\", 0.0)\n",
        "        p_val = p_values.get(f\"{src}-{dst}\", 1.0)\n",
        "\n",
        "        stars = get_star_label_from_p(p_val)\n",
        "        color = get_edge_color_from_p(p_val)\n",
        "        label = f\"β = {beta:.2f}{stars}, p = {p_val:.3f}\"\n",
        "\n",
        "        G.add_edge(src, dst, weight=beta, color=color, label=label)\n",
        "\n",
        "    # Draw figure\n",
        "    plt.figure(figsize=(12, 8))\n",
        "    nx.draw_networkx_nodes(G, pos, node_size=4000, node_color=\"lightblue\")\n",
        "\n",
        "    edge_colors = [G[u][v]['color'] for u, v in G.edges()]\n",
        "    nx.draw_networkx_edges(G, pos, edge_color=edge_colors, arrows=True)\n",
        "\n",
        "    nx.draw_networkx_labels(G, pos, font_size=10)\n",
        "    edge_labels = nx.get_edge_attributes(G, 'label')\n",
        "    nx.draw_networkx_edge_labels(G, pos, edge_labels=edge_labels, font_size=10)\n",
        "\n",
        "    # Add direct effect (Narcissism -> DevianceFreq) as a dashed arrow\n",
        "    direct_src, direct_dst = \"Narcissism\", \"DevianceFreq\"\n",
        "    direct_beta = beta_values.get(f\"{direct_src}-{direct_dst}\", 0.0)\n",
        "    direct_p = p_values.get(f\"{direct_src}-{direct_dst}\", 1.0)\n",
        "\n",
        "    direct_stars = get_star_label_from_p(direct_p)  # should be \"\" if p=0.415\n",
        "\n",
        "    src_x, src_y = pos[direct_src]\n",
        "    dst_x, dst_y = pos[direct_dst]\n",
        "\n",
        "    plt.annotate(\"\",\n",
        "                 xy=(dst_x, dst_y),\n",
        "                 xytext=(src_x, src_y),\n",
        "                 arrowprops=dict(arrowstyle=\"->\",\n",
        "                                 color=\"blue\",\n",
        "                                 lw=1.5,\n",
        "                                 linestyle=\"dashed\",\n",
        "                                 connectionstyle=\"arc3,rad=-0.3\"))\n",
        "\n",
        "    mid_x = (src_x + dst_x) / 2\n",
        "    mid_y = (src_y + dst_y) / 2 + 0.3\n",
        "\n",
        "    # No star for p=0.415, so it will just show \"β = 0.05, p = 0.415\"\n",
        "    plt.text(mid_x, mid_y,\n",
        "             f\"β = {direct_beta:.2f}{direct_stars}, p = {direct_p:.3f}\",\n",
        "             fontsize=10, color=\"blue\", ha=\"center\")\n",
        "\n",
        "    plt.title(\"Mediation Model with Narcissism\")\n",
        "    plt.axis(\"off\")\n",
        "    plt.savefig(save_path, dpi=300)\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "# Corrected data for the Narcissism model\n",
        "beta_values_narcissism = {\n",
        "    \"Narcissism-Resource Constraints\":    0.15,\n",
        "    \"Narcissism-Quick Results\":           0.38,\n",
        "    \"Narcissism-Avoid Clarification\":     0.23,\n",
        "    \"Resource Constraints-DevianceFreq\":  0.18,\n",
        "    \"Quick Results-DevianceFreq\":         0.25,\n",
        "    \"Avoid Clarification-DevianceFreq\":   0.15,\n",
        "    # Direct effect\n",
        "    \"Narcissism-DevianceFreq\":           0.05\n",
        "}\n",
        "\n",
        "p_values_narcissism = {\n",
        "    \"Narcissism-Resource Constraints\":    0.002,\n",
        "    \"Narcissism-Quick Results\":           0.001,\n",
        "    \"Narcissism-Avoid Clarification\":     0.001,\n",
        "    \"Resource Constraints-DevianceFreq\":  0.002,\n",
        "    \"Quick Results-DevianceFreq\":         0.002,\n",
        "    \"Avoid Clarification-DevianceFreq\":   0.037,\n",
        "    # Direct effect\n",
        "    \"Narcissism-DevianceFreq\":           0.415\n",
        "}\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    create_narcissism_mediation_diagram(\n",
        "        beta_values_narcissism,\n",
        "        p_values_narcissism,\n",
        "        save_path=\"mediation_model_narcissism.png\"\n",
        "    )\n"
