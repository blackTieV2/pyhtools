      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def create_narcissism_mediation_diagram(beta_values, p_values, save_path=\"mediation_model_narcissism.png\"):\n",
        "    \"\"\"\n",
        "    Generate a mediation diagram for Narcissism.\n",
        "    \"\"\"\n",
        "    G = nx.DiGraph()\n",
        "\n",
        "    # Node positions\n",
        "    pos = {\n",
        "        \"Narcissism\": (-2, 0),\n",
        "        \"Resource Constraints\": (0, 1),\n",
        "        \"Risk-Taking\": (0, 0),\n",
        "        \"Avoiding Clarification\": (0, -1),\n",
        "        \"DevianceFreq\": (2, 0)\n",
        "    }\n",
        "    G.add_nodes_from(pos.keys())\n",
        "\n",
        "    # Edges for mediator paths\n",
        "    mediator_edges = [\n",
        "        (\"Narcissism\", \"Resource Constraints\"),\n",
        "        (\"Narcissism\", \"Risk-Taking\"),\n",
        "        (\"Narcissism\", \"Avoiding Clarification\"),\n",
        "        (\"Resource Constraints\", \"DevianceFreq\"),\n",
        "        (\"Risk-Taking\", \"DevianceFreq\"),\n",
        "        (\"Avoiding Clarification\", \"DevianceFreq\")\n",
        "    ]\n",
        "\n",
        "    def get_star_label(beta):\n",
        "        \"\"\" Return a star label based on the beta magnitude. \"\"\"\n",
        "        if abs(beta) > 0.20:\n",
        "            return \"***\"\n",
        "        elif abs(beta) >= 0.10:\n",
        "            return \"**\"\n",
        "        else:\n",
        "            return \"*\"\n",
        "\n",
        "    # Add edges with labels\n",
        "    for (src, dst) in mediator_edges:\n",
        "        beta = beta_values.get(f\"{src}-{dst}\", 0)\n",
        "        p_val = p_values.get(f\"{src}-{dst}\", 1)\n",
        "\n",
        "        color = \"green\" if p_val < 0.01 else \"orange\" if p_val < 0.05 else \"gray\"\n",
        "        star_label = get_star_label(beta)\n",
        "\n",
        "        G.add_edge(src, dst, weight=beta, color=color, label=f\"β = {beta:.2f}{star_label}, p = {p_val:.3f}\")\n",
        "\n",
        "    plt.figure(figsize=(12, 8))\n",
        "\n",
        "    # Draw nodes\n",
        "    nx.draw_networkx_nodes(G, pos, node_size=4000, node_color=\"lightblue\")\n",
        "\n",
        "    # Draw edges\n",
        "    edge_colors = [G[u][v]['color'] for u, v in G.edges()]\n",
        "    nx.draw_networkx_edges(G, pos, edge_color=edge_colors, arrows=True)\n",
        "\n",
        "    # Node labels\n",
        "    nx.draw_networkx_labels(G, pos, font_size=10)\n",
        "\n",
        "    # Edge labels\n",
        "    edge_labels = nx.get_edge_attributes(G, 'label')\n",
        "    nx.draw_networkx_edge_labels(G, pos, edge_labels=edge_labels, font_size=10)\n",
        "\n",
        "    # Add direct effect arrow\n",
        "    direct_src, direct_dst = \"Narcissism\", \"DevianceFreq\"\n",
        "    direct_beta = beta_values[\"Narcissism-DevianceFreq\"]\n",
        "    direct_p = p_values[\"Narcissism-DevianceFreq\"]\n",
        "\n",
        "    psy_x, psy_y = pos[direct_src]\n",
        "    dev_x, dev_y = pos[direct_dst]\n",
        "\n",
        "    plt.annotate(\"\", xy=(dev_x, dev_y), xytext=(psy_x, psy_y),\n",
        "                 arrowprops=dict(arrowstyle=\"->\", color=\"blue\", lw=1.5, linestyle=\"dashed\",\n",
        "                                 connectionstyle=\"arc3,rad=-0.3\"))\n",
        "\n",
        "    mid_x = (psy_x + dev_x) / 2\n",
        "    mid_y = (psy_y + dev_y) / 2 + 0.3\n",
        "\n",
        "    plt.text(mid_x, mid_y, f\"β = {direct_beta:.2f}{get_star_label(direct_beta)}, p = {direct_p:.3f}\",\n",
        "             fontsize=10, color=\"blue\", ha=\"center\")\n",
        "\n",
        "    plt.title(\"Mediation Model with Narcissism\")\n",
        "    plt.axis(\"off\")\n",
        "    plt.savefig(save_path, dpi=300)\n",
        "    plt.show()\n",
        "\n",
        "\n",
        "# Beta and p-values for Narcissism based on the PDF results\n",
        "beta_values = {\n",
        "    \"Narcissism-Resource Constraints\": 0.15,\n",
        "    \"Narcissism-Risk-Taking\": 0.38,\n",
        "    \"Narcissism-Avoiding Clarification\": 0.23,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.18,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.25,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.15,\n",
        "    \"Narcissism-DevianceFreq\": 0.05  # direct effect\n",
        "}\n",
        "\n",
        "p_values = {\n",
        "    \"Narcissism-Resource Constraints\": 0.002,\n",
        "    \"Narcissism-Risk-Taking\": 0.001,\n",
        "    \"Narcissism-Avoiding Clarification\": 0.001,\n",
        "    \"Resource Constraints-DevianceFreq\": 0.002,\n",
        "    \"Risk-Taking-DevianceFreq\": 0.002,\n",
        "    \"Avoiding Clarification-DevianceFreq\": 0.037,\n",
        "    \"Narcissism-DevianceFreq\": 0.415  # direct effect\n",
        "}\n",
        "\n",
        "# Generate the refined Narcissism mediation diagram\n",
        "create_narcissism_mediation_diagram(beta_values, p_values)\n"
